{
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "framework": null,
  "git": {
    "deploymentEnabled": {
      "master": true
    }
  },
  "cleanUrls": true,
  "trailingSlash": true,
  "rewrites": [
    {
      "source": "/health",
      "destination": "/health"
    },
    {
      "source": "/api/:path*",
      "destination": "https://api-shopflow.railway.app/:path*"
    },
    {
      "source": "/shopflow/:path*",
      "destination": "https://shopflow.vercel.app/:path*"
    },
    {
      "source": "/workify/:path*",
      "destination": "https://workify.vercel.app/:path*"
    }
  ]
}
